{
  "name": "@cross-platform/taro-app",
  "version": "1.0.0",
  "description": "Taro多端应用",
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build": "npm run build:weapp && npm run build:h5",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev": "npm run dev:weapp",
    "test": "vitest",
    "test:sandbox": "vitest run",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.15",
    "@tarojs/components": "3.6.15",
    "@tarojs/helper": "3.6.15",
    "@tarojs/plugin-framework-react": "3.6.15",
    "@tarojs/plugin-platform-alipay": "3.6.15",
    "@tarojs/plugin-platform-h5": "3.6.15",
    "@tarojs/plugin-platform-jd": "3.6.15",
    "@tarojs/plugin-platform-qq": "3.6.15",
    "@tarojs/plugin-platform-swan": "3.6.15",
    "@tarojs/plugin-platform-tt": "3.6.15",
    "@tarojs/plugin-platform-weapp": "3.6.15",
    "@tarojs/react": "3.6.15",
    "@tarojs/runtime": "3.6.15",
    "@tarojs/shared": "3.6.15",
    "@tarojs/taro": "3.6.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "taro-ui": "^3.3.0",
    "uuid": "^9.0.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.22.15",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.11",
    "@tarojs/cli": "3.6.15",
    "@tarojs/webpack5-runner": "3.6.15",
    "@types/node": "22.13.10",
    "@types/react": "^18.2.21",
    "@types/uuid": "^9.0.2",
    "@types/webpack-env": "1.18.1",
    "babel-preset-taro": "3.6.15",
    "eslint-config-taro": "3.6.15",
    "react-refresh": "0.14.0",
    "rimraf": "5.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "vite": "6.0.0",
    "vitest": "2.1.5",
    "webpack": "5.88.2"
  }
}