{
  "pipeline": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "ios-build": {
      "dependsOn": ["build"],
      "outputs": [],
      "env": ["APPLE_DEVELOPER_TEAM_ID"]
    },
    "test": {
      "outputs": [],
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "cache": false
    }
  }
}
